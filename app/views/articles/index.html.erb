<h1>Articles</h1>

<ul>
  <% @articles.each do |article| %>
    <li>
      <%= link_to article.title, article_path(article) %>
      <div>Body: <%= article.body %></div>
      <div>Created at: <%= article.created_at %></div>
      <div>Updated at: <%= article.updated_at %></div>
    </li>
  <% end %>
</ul>


